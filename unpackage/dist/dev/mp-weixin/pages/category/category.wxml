',[['swichsubNav',['$0'],[[['subtabbar','',index,'id']]]]]]]}}" catchtap="__e"><text>{{item.name}}</text></view></block></view></view></block><block><scroll-view class="right-box" style="{{'height:'+(height+'px')+';'+('top:'+(top+'px')+';')}}" scroll-y><view class="page-view"><view class="class-box"><view class="tui-product-box"><view class="tui-product-list"><block wx:for="{{productList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="tui-pro-item" hover-class="hover" hover-start-time="{{150}}" data-event-opts="{{[['tap',[['detail',['$0'],[[['productList','',index]]]]]]]}}" catchtap="__e"><view class="tui-pro-img-container"><image class="tui-pro-img" src="{{item.img_url[0]}}" mode="aspectFill"></image></view><view class="tui-pro-content"><view class="tui-pro-tit">{{item.name}}</view><view><view class="tui-pro-price"><text class="tui-sale-price">{{"￥"+item.price}}</text></view></view></view></view></block></view></view></view><tui-loadmore vue-id="2" visible="{{loadding}}" index="{{3}}" type="red" bind:__l="__l"></tui-loadmore><tui-nomore vue-id="3" visible="{{!pullUpOn}}" bgcolor="#fafafa" bind:__l="__l"></tui-nomore></view></scroll-view></block></view>