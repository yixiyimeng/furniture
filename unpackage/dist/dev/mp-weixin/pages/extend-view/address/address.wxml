<view class="tui-safe-area"><view class="tui-address"><block wx:for="{{addressList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><tui-list-cell vue-id="{{'bcda9e66-1-'+index}}" padding="0" bind:__l="__l" vue-slots="{{['default']}}"><view data-event-opts="{{[['tap',[['checkboxChange',['$0'],[[['addressList','',index]]]]]]]}}" class="tui-address-flex" bindtap="__e"><view class="tui-address-left"><view class="tui-address-main"><view class="tui-address-name tui-ellipsis">{{item.contact_name}}</view><view class="tui-address-tel">{{item.contact_phone}}</view></view><view class="tui-address-detail"><block wx:if="{{item.is_default===1}}"><view class="tui-address-label">默认</view></block><text>{{item.province+item.city+item.district+item.address}}</text></view></view><block wx:if="{{!ischooseAddr}}"><view class="tui-address-imgbox"><image class="tui-address-img " style="margin-right:40rpx;" src="/static/images/mall/my/icon_addr_edit.png" data-event-opts="{{[['tap',[['editAddr',['$0'],[[['addressList','',index,'id']]]]]]]}}" catchtap="__e"></image><image class="tui-address-img" src="/static/images/mall/my/icon_addr_delete.png" data-event-opts="{{[['tap',[['deleteAddress',['$0'],[[['addressList','',index,'id']]]]]]]}}" catchtap="__e"></image></view></block></view></tui-list-cell></block></block></view><view class="tui-address-new"><tui-button vue-id="bcda9e66-2" type="danger" height="88rpx" data-event-opts="{{[['^click',[['editAddr',['']]]]]}}" bind:click="__e" bind:__l="__l" vue-slots="{{['default']}}">+ 新增收货地址</tui-button></view></view>